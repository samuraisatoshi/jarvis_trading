system_startup:
  title: "\U0001F680 JARVIS TRADING SYSTEM ONLINE"
  emoji: "\U0001F680"
  priority: high
  fields:
  - symbol
  - timeframe
  - account_id
  - initial_balance
system_shutdown:
  title: "\U0001F6D1 SISTEMA PARADO"
  emoji: "\U0001F6D1"
  priority: high
  fields:
  - reason
  - final_balance
  - total_profit_loss
system_error:
  title: "\u26A0\uFE0F ERRO DETECTADO"
  emoji: "\u26A0\uFE0F"
  priority: critical
  fields:
  - error_type
  - error_message
  - context
  - timestamp
market_analysis:
  title: "\U0001F4CA AN\xC1LISE DE MERCADO"
  emoji: "\U0001F4CA"
  priority: medium
  fields:
  - symbol
  - price
  - rsi
  - macd
  - macd_signal
  - bb_position
  - volume_change
  thresholds:
    rsi_oversold: 30
    rsi_overbought: 70
    volume_threshold: 15
trade_signal:
  title: "\U0001F3AF SINAL DE TRADING"
  emoji: "\U0001F3AF"
  priority: high
  fields:
  - symbol
  - action
  - confidence
  - price
  - indicators
  - reasoning
  action_emojis:
    BUY: "\U0001F49A"
    SELL: "\u274C"
    HOLD: "\u23F8\uFE0F"
trade_executed:
  title: "\u2705 TRADE EXECUTADO"
  emoji: "\u2705"
  priority: high
  fields:
  - trade_type
  - symbol
  - quantity
  - price
  - total_cost
  - new_balance_usdt
  - new_balance_asset
  - total_value
circuit_breaker:
  title: "\U0001F6A8 CIRCUIT BREAKER ATIVADO"
  emoji: "\U0001F6A8"
  priority: critical
  fields:
  - reason
  - current_drawdown
  - max_drawdown
  - action_required
  thresholds:
    max_drawdown: 0.15
    min_win_rate: 0.35
risk_alert:
  title: "\u26A0\uFE0F ALERTA DE RISCO"
  emoji: "\u26A0\uFE0F"
  priority: high
  fields:
  - alert_type
  - current_value
  - threshold
  - recommendation
daily_report:
  title: "\U0001F4CA RELAT\xD3RIO DI\xC1RIO"
  emoji: "\U0001F4CA"
  priority: medium
  fields:
  - date
  - trades_count
  - wins
  - losses
  - win_rate
  - profit_loss
  - profit_loss_pct
  - total_value
  schedule: 00:00
weekly_report:
  title: "\U0001F4C8 RELAT\xD3RIO SEMANAL"
  emoji: "\U0001F4C8"
  priority: medium
  fields:
  - week_start
  - week_end
  - trades_count
  - wins
  - losses
  - win_rate
  - profit_loss
  - profit_loss_pct
  - best_trade
  - worst_trade
  - total_value
  schedule: Monday 00:00
performance_update:
  title: "\U0001F4B9 ATUALIZA\xC7\xC3O DE PERFORMANCE"
  emoji: "\U0001F4B9"
  priority: low
  fields:
  - period
  - roi
  - sharpe_ratio
  - max_drawdown
  - win_rate
  - profit_factor
health_check:
  title: "\U0001F3E5 HEALTH CHECK"
  emoji: "\U0001F3E5"
  priority: low
  fields:
  - api_status
  - database_status
  - model_status
  - last_execution
  - next_execution
  - uptime
anomaly_detected:
  title: "\U0001F50D ANOMALIA DETECTADA"
  emoji: "\U0001F50D"
  priority: high
  fields:
  - anomaly_type
  - description
  - severity
  - recommendation
  types:
  - volume_spike
  - price_gap
  - volatility_surge
  - correlation_break
formatting:
  parse_mode: HTML
  disable_web_page_preview: true
  disable_notification: false
rate_limiting:
  max_messages_per_minute: 30
  retry_attempts: 3
  retry_delay_seconds: 2
notifications:
  startup: true
  shutdown: true
  market_analysis: true
  trade_signals: true
  trade_executions: true
  circuit_breaker: true
  daily_reports: true
  weekly_reports: true
  errors: true
  warnings: true
  health_checks: false
security:
  require_authorization: true
  whitelist_enabled: true
